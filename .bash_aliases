
alias 'x'='clear'
alias 'su+'='sudo -Ei'
alias 'ls'="/usr/bin/ls ${LS_OPTIONS} -h"
alias 'll'="/usr/bin/ls ${LS_OPTIONS} -hl"
alias 'la'="/usr/bin/ls ${LS_OPTIONS} -hla"
alias 'rm'='rm --preserve-root -v -I'
alias 'chown'='chown --preserve-root -v'
alias 'chmod'='chmod --preserve-root -v'
alias 'mkdir'='mkdir -p -v'
alias 'grep'='grep --color=auto'
alias 'cp'='cp -r -v'
alias 'mv'='mv -v'
alias 'emacs'='emacs -nw'
alias '..'='cd ..'
alias 'fucking'='sudo'
alias 'weather'='curl http://wttr.in/Kyiv?lang=uk' 2> /dev/null
alias 'KILL'='killall -s KILL'

